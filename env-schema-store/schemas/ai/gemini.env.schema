# Google Gemini AI Configuration
# @package @google/generative-ai
# @version ^0.7.0
# @url https://ai.google.dev/docs

# === API Keys ===

# @required @secret
# @desc Google AI Studio API key
# @pattern ^[a-zA-Z0-9_-]{39}$
GOOGLE_GENERATIVE_AI_API_KEY=

# @optional @secret
# @desc Alternative: Gemini API key
# @pattern ^[a-zA-Z0-9_-]{39}$
GEMINI_API_KEY=

# === Model Configuration ===

# @optional @public
# @desc Gemini model to use
# @default gemini-pro
# @enum gemini-pro gemini-pro-vision gemini-1.5-pro gemini-1.5-flash
GEMINI_MODEL=

# @optional @public @number
# @desc Temperature for generation
# @default 0.7
# @min 0
# @max 2
GEMINI_TEMPERATURE=

# @optional @public @number
# @desc Maximum output tokens
# @default 2048
# @min 1
# @max 32768
GEMINI_MAX_OUTPUT_TOKENS=

# @optional @public @number
# @desc Top-K sampling
# @default 40
# @min 1
# @max 100
GEMINI_TOP_K=

# @optional @public @number
# @desc Top-P (nucleus) sampling
# @default 0.95
# @min 0
# @max 1
GEMINI_TOP_P=

# === Safety Settings ===

# @optional @public
# @desc Harassment blocking threshold
# @default BLOCK_MEDIUM_AND_ABOVE
# @enum BLOCK_NONE BLOCK_LOW_AND_ABOVE BLOCK_MEDIUM_AND_ABOVE BLOCK_ONLY_HIGH
GEMINI_HARM_BLOCK_HARASSMENT=

# @optional @public
# @desc Hate speech blocking threshold
# @default BLOCK_MEDIUM_AND_ABOVE
# @enum BLOCK_NONE BLOCK_LOW_AND_ABOVE BLOCK_MEDIUM_AND_ABOVE BLOCK_ONLY_HIGH
GEMINI_HARM_BLOCK_HATE_SPEECH=

# @optional @public
# @desc Sexually explicit blocking threshold
# @default BLOCK_MEDIUM_AND_ABOVE
# @enum BLOCK_NONE BLOCK_LOW_AND_ABOVE BLOCK_MEDIUM_AND_ABOVE BLOCK_ONLY_HIGH
GEMINI_HARM_BLOCK_SEXUALLY_EXPLICIT=

# @optional @public
# @desc Dangerous content blocking threshold
# @default BLOCK_MEDIUM_AND_ABOVE
# @enum BLOCK_NONE BLOCK_LOW_AND_ABOVE BLOCK_MEDIUM_AND_ABOVE BLOCK_ONLY_HIGH
GEMINI_HARM_BLOCK_DANGEROUS_CONTENT=

# === API Configuration ===

# @optional @public
# @desc API endpoint base URL
# @default https://generativelanguage.googleapis.com
# @pattern ^https?:\/\/.+$
GEMINI_API_BASE_URL=

# @optional @public
# @desc API version
# @default v1beta
# @enum v1 v1beta
GEMINI_API_VERSION=

# @optional @public @number
# @desc Request timeout in milliseconds
# @default 60000
# @min 1000
GEMINI_TIMEOUT=

# === Vertex AI (Alternative) ===

# @optional @public
# @desc Google Cloud project ID
# @pattern ^[a-z][a-z0-9-]{4,28}[a-z0-9]$
GOOGLE_CLOUD_PROJECT=

# @optional @public
# @desc Google Cloud region
# @default us-central1
# @enum us-central1 us-east1 us-west1 europe-west1 asia-northeast1
GOOGLE_CLOUD_REGION=

# @optional @secret
# @desc Google Cloud service account key (JSON)
# @json
GOOGLE_APPLICATION_CREDENTIALS=

# === Cache Configuration ===

# @optional @public @boolean
# @desc Enable response caching
# @default false
GEMINI_CACHE_ENABLED=

# @optional @public @number
# @desc Cache TTL in seconds
# @default 3600
# @min 0
GEMINI_CACHE_TTL=

# === Debugging ===

# @optional @public @boolean
# @desc Enable debug logging
# @default false
GEMINI_DEBUG=

# @optional @public @boolean
# @desc Log API requests
# @default false
GEMINI_LOG_REQUESTS=

# @optional @public @boolean
# @desc Log API responses
# @default false
GEMINI_LOG_RESPONSES=